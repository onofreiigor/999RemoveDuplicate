{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "hello_extensions.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ]
}
